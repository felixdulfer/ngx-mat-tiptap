{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/ngx-mat-tiptap",
  "lib": {
    "entryFile": "src/public-api.ts"
  },
  "assets": [
    {
      "glob": "styles.css",
      "input": "src/",
      "output": "./"
    },
    {
      "glob": "**/*.json",
      "input": "schematics/",
      "output": "./schematics"
    },
    {
      "glob": "**/*.js",
      "input": "schematics/.schematics/",
      "output": "./schematics"
    },
    {
      "glob": "**/*.d.ts",
      "input": "schematics/.schematics/",
      "output": "./schematics"
    }
  ]
}
